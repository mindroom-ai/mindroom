# Matrix Server Details
MATRIX_HOMESERVER=http://localhost:8008
MATRIX_USER_ID=@your-bot:localhost
MATRIX_PASSWORD=your-bot-password

# Agno AI Configuration
AGNO_MODEL=ollama:devstral:24b

# AI Caching Configuration
# Set to false to disable caching (default: true)
ENABLE_AI_CACHE=true
# Directory for storing cached AI responses
AI_CACHE_DIR=tmp/.ai_cache

# API Keys (only needed if not using a local model like ollama)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
OLLAMA_HOST=http://pc.local:11434

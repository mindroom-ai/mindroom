# Dokku Server Configuration
DOKKU_HOST=your-dokku-server.com
DOKKU_USER=root
DOKKU_PORT=22
# Use either SSH key path or password authentication
DOKKU_SSH_KEY_PATH=/path/to/ssh/key
# Or
DOKKU_PASSWORD=your-ssh-password

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key

# Platform Configuration
PLATFORM_DOMAIN=mindroom.app
REGISTRY=ghcr.io/mindroom

# Service Configuration
DOKKU_PROVISIONER_PORT=8002
NODE_ENV=production
LOG_LEVEL=info

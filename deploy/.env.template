# Mindroom Instance Environment Template
# Copy this file to .env.<instance_name> and fill in your values

# API Keys (shared across instances)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
GROQ_API_KEY=

# Google OAuth (if using)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_PROJECT_ID=
GOOGLE_REDIRECT_URI=

# Other services (optional)
DEEPSEEK_API_KEY=
OPENROUTER_API_KEY=
OLLAMA_HOST=
REDIS_URL=

# Logging
LOG_LEVEL=INFO

# Domain configuration (if using Traefik)
DOMAIN=localhost

# Instance-specific variables will be appended by instance_manager.py:
# INSTANCE_NAME=
# BACKEND_PORT=
# FRONTEND_PORT=
# DATA_DIR=
# INSTANCE_DOMAIN=
